<h1>New Instrument for Artist</h1>

<%= form_for @artist_instrument do |f| %>
<%= f.label :artist_id, 'Artist' %>
<%= f.collection_select :artist_id, Artist.all, :id, :name %>

<%= f.label :instrument_id, 'Instrument' %>
<%= f.collection_select :instrument_id, Instrument.all, :id, :name %>

<%= f.submit %>

<% end %>